<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.headlit.android.calendar100">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/calender100icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Calendar100">
        <activity android:name="com.headlit.android.calendar100.ResultActivity"></activity>
        <activity android:name="com.headlit.android.calendar100.DateActivity" />
        <activity android:name="com.headlit.android.calendar100.MonthActivity" />
        <activity android:name="com.headlit.android.calendar100.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>